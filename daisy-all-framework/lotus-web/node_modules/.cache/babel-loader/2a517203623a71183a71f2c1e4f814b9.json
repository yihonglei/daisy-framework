 $table.doLayout()\n}\n\nexport default {\n  bind(el, binding, vnode) {\n    el.resizeListener = () => {\n      doResize(el, binding, vnode)\n    }\n    // parameter 1 is must be \"Element\" type\n    addResizeListener(window.document.body, el.resizeListener)\n  },\n  inserted(el, binding, vnode) {\n    doResize(el, binding, vnode)\n  },\n  unbind(el) {\n    removeResizeListener(window.document.body, el.resizeListener)\n  }\n}\n"],"mappings":"AAAA,SAASA,iBAAT,EAA4BC,oBAA5B,QAAwD,mCAAxD;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA,IAAMC,QAAQ,GAAG,SAAXA,QAAW,CAACC,EAAD,EAAKC,OAAL,EAAcC,KAAd,EAAwB;EACvC,IAA2BC,MAA3B,GAAsCD,KAAtC,CAAQE,iBAAR;EAEA,IAAQC,KAAR,GAAkBJ,OAAlB,CAAQI,KAAR;;EAEA,IAAI,CAACF,MAAM,CAACG,MAAZ,EAAoB;IAClB,MAAM,IAAIC,KAAJ,yDAAN;EACD;;EACD,IAAMC,YAAY,GAAIH,KAAK,IAAIA,KAAK,CAACG,YAAhB,IAAiC,EAAtD;EAEA,IAAI,CAACL,MAAL,EAAa;EAEb,IAAMG,MAAM,GAAGG,MAAM,CAACC,WAAP,GAAqBV,EAAE,CAACW,qBAAH,GAA2BC,GAAhD,GAAsDJ,YAArE;EACAL,MAAM,CAACU,MAAP,CAAcC,SAAd,CAAwBR,MAAxB;EACAH,MAAM,CAACY,QAAP;AACD,CAfD;;AAiBA,eAAe;EACbC,IADa,gBACRhB,EADQ,EACJC,OADI,EACKC,KADL,EACY;IACvBF,EAAE,CAACiB,cAAH,GAAoB,YAAM;MACxBlB,QAAQ,CAACC,EAAD,EAAKC,OAAL,EAAcC,KAAd,CAAR;IACD,CAFD,CADuB,CAIvB;;;IACAL,iBAAiB,CAACY,MAAM,CAACS,QAAP,CAAgBC,IAAjB,EAAuBnB,EAAE,CAACiB,cAA1B,CAAjB;EACD,CAPY;EAQbG,QARa,oBAQJpB,EARI,EAQAC,OARA,EAQSC,KART,EAQgB;IAC3BH,QAAQ,CAACC,EAAD,EAAKC,OAAL,EAAcC,KAAd,CAAR;EACD,CAVY;EAWbmB,MAXa,kBAWNrB,EAXM,EAWF;IACTF,oBAAoB,CAACW,MAAM,CAACS,QAAP,CAAgBC,IAAjB,EAAuBnB,EAAE,CAACiB,cAA1B,CAApB;EACD;AAbY,CAAf"}]}