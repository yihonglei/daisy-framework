op = this.$el.getBoundingClientRect().top\n      if (offsetTop < this.stickyTop) {\n        this.sticky()\n        return\n      }\n      this.handleReset()\n    },\n    handleResize() {\n      if (this.isSticky) {\n        this.width = this.$el.getBoundingClientRect().width + 'px'\n      }\n    }\n  }\n}\n</script>\n"]}]}