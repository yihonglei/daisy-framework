\r\n      accessRoutes.push(route)\r\n    } else {\r\n      // 保存普通路由\r\n      accessRoutes.push(getRoute(routes[i]))\r\n    }\r\n  }\r\n  // 返回路由结果\r\n  return accessRoutes\r\n}\r\n\r\n// 返回路由的基本格式\r\nfunction getRoute(item) {\r\n  // 路由基本格式\r\n  let route = {}\r\n  if (item.parent_id == 0) {\r\n    route = {\n      hidden: item.hidden,\n      redirect: item.redirect,\n      alwaysShow: item.alwaysShow,\n      // 路由名\n      name: item.name,\r\n      // 路由的路径\r\n      path: item.path,\r\n      // 路由所在组件 @/layout\r\n      component: (resolve) => require([`@/layout`], resolve),\r\n      // component: (resolve) => require([`@/views${item.component}`], resolve),\r\n      meta: {\n        title: item.title,\r\n        icon: item.icon,\r\n        noCache: item.noCache,\n        breadcrumb: item.breadcrumb,\r\n        affix: item.affix,\r\n        activeMenu: item.activeMenu\r\n      }\r\n    }\r\n  } else {\r\n    route = {\r\n      // 路由的路径\r\n      path: item.path,\r\n      // 路由名\r\n      name: item.name,\r\n      alwaysShow: item.alwaysShow,\r\n      hidden: item.hidden,\r\n      redirect: item.redirect,\r\n      // 路由所在组件 @/layout\r\n      component: (resolve) => require([`@/views${item.component}`], resolve),\r\n      meta: {\r\n        icon: item.icon,\r\n        title: item.title,\r\n        affix: item.affix,\r\n        breadcrumb: item.hidden_breadcrumb,\r\n      }\r\n    }\r\n  }\r\n\r\n  // 返回 route\r\n  return route\r\n}\r\n\r\n\r\nconst state = {\r\n  routes: [],\r\n  addRoutes: []\r\n}\r\n\r\nconst mutations = {\r\n  SET_ROUTES: (state, routes) => {\r\n    state.addRoutes = routes\r\n    state.routes = constantRoutes.concat(routes)\r\n  }\r\n}\r\n\r\nconst actions = {\r\n  generateRoutes({\r\n    commit\r\n  }, routes) {\r\n    return new Promise(resolve => {\r\n\r\n      let accessedRoutes = fnAddDynamicMenuRoutes(routes)\r\n      commit('SET_ROUTES', accessedRoutes)\r\n      resolve(accessedRoutes)\r\n    })\r\n  }\r\n}\r\n\r\nexport default {\r\n  namespaced: true,\r\n  state,\r\n  mutations,\r\n  actions\r\n}\n"],"mappings":";;;AAAA,SACEA,WADF,EAEEC,cAFF,QAGO,UAHP,C,CAKA;;AACA,OAAO,SAASC,sBAAT,GAA6C;EAAA,IAAbC,MAAa,uEAAJ,EAAI;EAClD,IAAIC,YAAY,GAAG,EAAnB,CADkD,CAElD;;EACA,IAAIC,IAAI,GAAG,EAAX,CAHkD,CAIlD;;EACA,IAAIC,KAAK,GAAG,EAAZ,CALkD,CAMlD;;EACA,KAAK,IAAIC,CAAC,GAAG,CAAb,EAAgBA,CAAC,GAAGJ,MAAM,CAACK,MAA3B,EAAmCD,CAAC,EAApC,EAAwC;IACtC;IACA,IAAIJ,MAAM,CAACI,CAAD,CAAN,CAAUE,QAAV,IAAsBN,MAAM,CAACI,CAAD,CAAN,CAAUE,QAAV,CAAmBD,MAAnB,GAA4B,CAAtD,EAAyD;MACvD;MACAF,KAAK,GAAGI,QAAQ,CAACP,MAAM,CAACI,CAAD,CAAP,CAAhB,CAFuD,CAGvD;;MACAD,KAAK,CAACG,QAAN,GAAiBP,sBAAsB,CAACC,MAAM,CAACI,CAAD,CAAN,CAAUE,QAAX,CAAvC,CAJuD,CAKvD;;MACAL,YAAY,CAACO,IAAb,CAAkBL,KAAlB;IACD,CAPD,MAOO;MACL;MACAF,YAAY,CAACO,IAAb,CAAkBD,QAAQ,CAACP,MAAM,CAACI,CAAD,CAAP,CAA1B;IACD;EACF,CApBiD,CAqBlD;;;EACA,OAAOH,YAAP;AACD,C,CAED;;AACA,SAASM,QAAT,CAAkBE,IAAlB,EAAwB;EACtB;EACA,IAAIN,KAAK,GAAG,EAAZ;;EACA,IAAIM,IAAI,CAACC,SAAL,IAAkB,CAAtB,EAAyB;IACvBP,KAAK,GAAG;MACNQ,MAAM,EAAEF,IAAI,CAACE,MADP;MAENC,QAAQ,EAAEH,IAAI,CAACG,QAFT;MAGNC,UAAU,EAAEJ,IAAI,CAACI,UAHX;MAIN;MACAC,IAAI,EAAEL,IAAI,CAACK,IALL;MAMN;MACAC,IAAI,EAAEN,IAAI,CAACM,IAPL;MAQN;MACAC,SAAS,EAAE,mBAACC,OAAD;QAAA,OAAaC,OAAO,CAAC,YAAD,EAAeD,OAAf,CAApB;MAAA,CATL;MAUN;MACAE,IAAI,EAAE;QACJC,KAAK,EAAEX,IAAI,CAACW,KADR;QAEJC,IAAI,EAAEZ,IAAI,CAACY,IAFP;QAGJC,OAAO,EAAEb,IAAI,CAACa,OAHV;QAIJC,UAAU,EAAEd,IAAI,CAACc,UAJb;QAKJC,KAAK,EAAEf,IAAI,CAACe,KALR;QAMJC,UAAU,EAAEhB,IAAI,CAACgB;MANb;IAXA,CAAR;EAoBD,CArBD,MAqBO;IACLtB,KAAK,GAAG;MACN;MACAY,IAAI,EAAEN,IAAI,CAACM,IAFL;MAGN;MACAD,IAAI,EAAEL,IAAI,CAACK,IAJL;MAKND,UAAU,EAAEJ,IAAI,CAACI,UALX;MAMNF,MAAM,EAAEF,IAAI,CAACE,MANP;MAONC,QAAQ,EAAEH,IAAI,CAACG,QAPT;MAQN;MACAI,SAAS,EAAE,mBAACC,OAAD;QAAA,OAAaC,OAAO,CAAC,kBAAWT,IAAI,CAACO,SAAhB,EAAD,EAA+BC,OAA/B,CAApB;MAAA,CATL;MAUNE,IAAI,EAAE;QACJE,IAAI,EAAEZ,IAAI,CAACY,IADP;QAEJD,KAAK,EAAEX,IAAI,CAACW,KAFR;QAGJI,KAAK,EAAEf,IAAI,CAACe,KAHR;QAIJD,UAAU,EAAEd,IAAI,CAACiB;MAJb;IAVA,CAAR;EAiBD,CA1CqB,CA4CtB;;;EACA,OAAOvB,KAAP;AACD;;AAG