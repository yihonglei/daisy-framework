ide\n    </el-button>\n  </div>\n</template>\n\n<script>\nimport Driver from 'driver.js' // import driver.js\nimport 'driver.js/dist/driver.min.css' // import driver.js css\nimport steps from './steps'\n\nexport default {\n  name: 'Guide',\n  data() {\n    return {\n      driver: null\n    }\n  },\n  mounted() {\n    this.driver = new Driver()\n  },\n  methods: {\n    guide() {\n      this.driver.defineSteps(steps)\n      this.driver.start()\n    }\n  }\n}\n</script>\n"]}]}